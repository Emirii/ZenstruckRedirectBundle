    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="zenstruck_redirect.controller.class">Zenstruck\Bundle\RedirectBundle\Controller\RedirectController</parameter>
    </parameters>

    <services>
        <service id="zenstruck_redirect.controller" class="%zenstruck_redirect.controller.class%">
