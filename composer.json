{
    "name": "zenstruck/redirect-bundle",
    "description": "Store redirects for your site and keeps statistics on redirects and 404 errors",
    "keywords": ["redirect"],
    "homepage": "http://zenstruck.com/project/ZenstruckRedirectBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "symfony/framework-bundle": "~2.3|~3.0",
        "symfony/doctrine-bridge": "~2.3|~3.0",
        "doctrine/doctrine-bundle": "~1.3",
        "doctrine/orm": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.16",
        "matthiasnoback/symfony-dependency-injection-test": "^0.7.4",
        "symfony/finder": "~2.3|~3.0",
        "symfony/browser-kit": "~2.3|~3.0",
        "symfony/form": "~2.3|~3.0",
        "symfony/validator": "~2.3|~3.0"
    },
    "autoload": {
        "psr-4": { "Zenstruck\\RedirectBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Zenstruck\\RedirectBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
